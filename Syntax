--Basic Syntax for SQL

SELECT
  field1
  
FROM
   table
   
 WHERE
    field1 = condition;
    
    
 SELECT * -- Denotes All  Columns
 
SELECT
  City
  
FROM
   Texas
   
 WHERE
    Population > 80000;
    
    
--Example for SES and Vaccine Status of Texas Counties
SELECT
    County_Name,
    Population,
    Median_Income
FROM
    Texas_Counties
WHERE
    Vaccination_Rate > 0.4;


